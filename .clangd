CompileFlags:
  Add: [-std=c2x,
        -xc,
        -mcpu=cortex-m0,
        -Wall,
        -Werror=implicit-function-declaration,
        -Werror=implicit-int,
        -Werror=return-type,
        -Werror=strict-prototypes,
        -Werror=deprecated-non-prototype
        -Wextra,
        -Wimplicit-fallthrough,
        -Wpedantic,
        -fstack-protector-strong
      ]
  Compiler: arm-none-eabi-gcc
